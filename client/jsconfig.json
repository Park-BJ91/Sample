{
  "compilerOptions": {
    "baseUrl": "src",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@root/*": [
        "*"
      ],
      "@models/*": [
        "models/*"
      ]
    }
  }
}